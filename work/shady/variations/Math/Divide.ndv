<Shady>
	<Variation desc="Divides two variables to produce a quotient." simulationCommand="divide">
		<Input type="any" name="numerator"/>
		<Input type="any" name="denominator"/>
		<Output type="any" name="factor"/>
		<Source type="GLSL">
			<Code output="factor">
				[output] = numerator / denominator;
			</Code>
		</Source>
	</Variation>
</Shady>