<Shady>
	<Variation desc="Subtracts from input1 the value provided in input2." simulationCommand="subtract">
		<Input type="any" name="input1"/>
		<Input type="any" name="input2"/>
		<Output type="any" name="output"/>
		<Source type="GLSL">
			<Code output="output">
				[output] = input1 - input2;
			</Code>
		</Source>
	</Variation>
</Shady>