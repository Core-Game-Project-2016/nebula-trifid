<Shady>
	<Variation desc="Normalizes any vector." simulationCommand="normalize">
		<Input type="anyvec" name="input" inference="false"/>
		<Output type="anyvec" name="normalized"/>
		<Source type="GLSL">
			<Code output="normalized">
				[output] = normalize(input);
			</Code>
		</Source>
	</Variation>
</Shady>