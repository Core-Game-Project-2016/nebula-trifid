<Shady>
	<Variation desc="Takes two individual floats and converts to 2 component vector." simulationCommand="combine2">
		<Input type="float" name="Red"/>
		<Input type="float" name="Green"/>
		<Output type="vec2" name="output"/>
		<Source type="GLSL">
			<Code output="output">
				[output] = vec2(Red, Green);
			</Code>
		</Source>
	</Variation>
</Shady>