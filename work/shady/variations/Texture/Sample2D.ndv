<Shady>
	<Variation desc="Samples an RGBA 2D texture using default UVs." simulationValue="Source">
		<Output type="vec4" name="Sample"/>
		<Hidden type="sampler2D" name="Source"/>
		<Source type="GLSL">
			<Code output="Sample">
				[output] = texture(Source, params.uv);
			</Code>
		</Source>
	</Variation>
</Shady>
